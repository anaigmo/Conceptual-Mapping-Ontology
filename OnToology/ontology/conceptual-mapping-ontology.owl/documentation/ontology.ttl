#################################################################
#    Object Properties
#################################################################

###  http://purl.org/vocab/vann/hasSecurityScheme
<http://purl.org/vocab/vann/hasSecurityScheme> rdf:type owl:ObjectProperty ;
                                               rdfs:domain :DataAccess ;
                                               rdfs:range <https://www.w3.org/2019/wot/security#SecurityScheme> ;
                                               rdfs:label "has security scheme"@en ;
                                               <http://www.w3.org/2004/02/skos/core#definition> "A relation between a data access and the security scheme it has"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasArgument
:hasArgument rdf:type owl:ObjectProperty ;
             rdfs:domain :DataAccess ;
             rdfs:range :Argument ;
             rdfs:label "has argument"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> "A relation between the data access and an argument that describes it"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasAssociatedOntology
:hasAssociatedOntology rdf:type owl:ObjectProperty ;
                       rdfs:domain :StatementMap ;
                       rdfs:range :Ontology ;
                       rdfs:label "has associated ontology"@en ;
                       <http://www.w3.org/2004/02/skos/core#definition> "A relation between a mapping and the ontology of provenance"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasDataAccess
:hasDataAccess rdf:type owl:ObjectProperty ;
               rdfs:domain :DataSource ;
               rdfs:range :DataAccess ;
               rdfs:label "has data access"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "A relation between a data source and its data access"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasDataSource
:hasDataSource rdf:type owl:ObjectProperty ;
               rdfs:domain :DataSourceAccess ;
               rdfs:range :DataSource ;
               rdfs:label "has data source"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "A relation between a data source access and its data source"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasDataType
:hasDataType rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :StatementMap ;
             rdfs:range :ReferenceNode ;
             rdfs:label "has data type"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> "A relation between a statement map and a reference node to define the predicate"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasEvaluableExpression
:hasEvaluableExpression rdf:type owl:ObjectProperty ;
                        rdfs:domain :ResourceMap ;
                        rdfs:range :EvaluableExpression ;
                        rdfs:label "has evaluable expression" ;
                        <http://www.w3.org/2004/02/skos/core#definition> "A relation between a resource map and the evaluable expression that defines it"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasFunctionName
:hasFunctionName rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :RecursiveExpression ;
                 rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                     owl:hasValue <http://vocab.linkeddata.es/kos/conceptual-mapping#function-taxonomy>
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
                 rdfs:label "has function name"@en ;
                 <http://www.w3.org/2004/02/skos/core#definition> "A relation between a recursive expression and the name of the function it defines"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasInput
:hasInput rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :EvaluableExpression ;
          rdfs:range rdf:List ;
          rdfs:label "has input"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> "A relation between an evaluable expression and the list of parameters it requires"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasLanguage
:hasLanguage rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :StatementMap ;
             rdfs:range rdfs:Literal ;
             rdfs:label "has language"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> "A relation between a statement map and a literal to define the language"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasNamedGraph
:hasNamedGraph rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :StatementMap ;
               rdfs:range :Node ;
               rdfs:label "has named graph"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "A relation between a statement map and a node to define the assigned named graph"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasObject
:hasObject rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf rdf:object ;
           rdfs:domain :StatementMap ;
           rdfs:range :ResourceMap ;
           rdfs:label "has object"@en ;
           <http://www.w3.org/2004/02/skos/core#definition> "A relation between a statement map and a resource map to define the predicate"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasPredicate
:hasPredicate rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf rdf:predicate ;
              rdfs:domain :StatementMap ;
              rdfs:range :ReferenceNode ;
              rdfs:label "has predicate"@en ;
              <http://www.w3.org/2004/02/skos/core#definition> "A relation between a statement map and a reference node to define the predicate"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasProtocol
:hasProtocol rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :DataAccess ;
             rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                 owl:hasValue <http://vocab.linkeddata.es/kos/conceptual-mapping#protocol-taxonomy>
                                               ]
                                             ) ;
                          rdf:type owl:Class
                        ] ;
             rdfs:label "has protocol"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> "A relation between the data access and the protocol that describes it"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#hasSubject
:hasSubject rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf rdf:subject ;
            rdfs:domain :StatementMap ;
            rdfs:range :Node ;
            rdfs:label "has subject"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "A relation between a statement map and a node to define the subject"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#name
:name rdf:type owl:ObjectProperty ;
      rdfs:domain :Argument ;
      rdfs:range rdfs:Resource ;
      rdfs:label "name"@en ;
      <http://www.w3.org/2004/02/skos/core#definition> "A relation between an argument and the resource that describes its name"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#returns
:returns rdf:type owl:ObjectProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :EvaluableExpression ;
         rdfs:range rdfs:Resource ;
         rdfs:label "returns"@en ;
         <http://www.w3.org/2004/02/skos/core#definition> "A relation between an evaluable expression and the resource it returns"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#value
:value rdf:type owl:ObjectProperty ;
       rdfs:domain :Argument ;
       rdfs:range rdfs:Resource ;
       rdfs:label "value"@en ;
       <http://www.w3.org/2004/02/skos/core#definition> "A relation between an argument and the resource that describes its value"@en .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#object
rdf:object rdf:type owl:ObjectProperty ;
           rdfs:domain rdf:Statement ;
           rdfs:range rdfs:Resource ;
           rdfs:comment "The object of the subject RDF statement." ;
           rdfs:isDefinedBy rdf: ;
           rdfs:label "object" .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#predicate
rdf:predicate rdf:type owl:ObjectProperty ;
              rdfs:domain rdf:Statement ;
              rdfs:range rdfs:Resource ;
              rdfs:comment "The predicate of the subject RDF statement." ;
              rdfs:isDefinedBy rdf: ;
              rdfs:label "predicate" .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#subject
rdf:subject rdf:type owl:ObjectProperty ;
            rdfs:domain rdf:Statement ;
            rdfs:range rdfs:Resource ;
            rdfs:comment "The subject of the subject RDF statement." ;
            rdfs:isDefinedBy rdf: ;
            rdfs:label "subject" .


###  http://www.w3.org/2004/02/skos/core#inScheme
<http://www.w3.org/2004/02/skos/core#inScheme> rdf:type owl:ObjectProperty ;
                                               rdfs:subPropertyOf owl:topObjectProperty ;
                                               rdfs:range <http://www.w3.org/2004/02/skos/core#ConceptScheme> ;
                                               rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> ;
                                               rdfs:label "is in scheme"@en ;
                                               <http://www.w3.org/2004/02/skos/core#definition> "Relates a resource (for example a concept) to a concept scheme in which it is included."@en ;
                                               <http://www.w3.org/2004/02/skos/core#scopeNote> "A concept may be a member of more than one concept scheme."@en .


#################################################################
#    Data properties
#################################################################

###  http://vocab.linkeddata.es/def/conceptual-mapping#URI
:URI rdf:type owl:DatatypeProperty ;
     rdfs:domain :DataAccess ;
     rdfs:range xsd:anyURI ;
     rdfs:label "URI"@en ;
     <http://www.w3.org/2004/02/skos/core#definition> "Uniform Resource Identifier used to identify the resource retrieven by the data access"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#expression
:expression rdf:type owl:DatatypeProperty ;
            rdfs:domain :TextSource ;
            rdfs:range xsd:string ;
            rdfs:label "expression"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "What specifies which data is retrieven from a text source"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#iterator
:iterator rdf:type owl:DatatypeProperty ;
          rdfs:domain :TreeSource ;
          rdfs:range xsd:string ;
          rdfs:label "iterator"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> "What is to be iterated in a tree source"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#refreshTime
:refreshTime rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :AsynchronousSource ;
             rdfs:range xsd:integer ;
             rdfs:label "refresh time"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> "Time between every data request"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#separator
:separator rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :TabularSource ;
           rdfs:range xsd:string ;
           rdfs:label "separator"@en ;
           <http://www.w3.org/2004/02/skos/core#definition> "Character that separates two values from one field in a tabular source"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#shape
:shape rdf:type owl:DatatypeProperty ;
       rdfs:domain :Ontology ;
       rdfs:range xsd:anyURI ;
       rdfs:label "shape"@en ;
       <http://www.w3.org/2004/02/skos/core#definition> "Restrictions declared for an ontology in form of a shape"@en .


#################################################################
#    Classes
#################################################################

###  http://vocab.linkeddata.es/def/conceptual-mapping#Argument
:Argument rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :name ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass rdfs:Resource
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :value ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass rdfs:Resource
                          ] ;
          rdfs:label "Argument"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> "Required argument to describe data access"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#AsynchronousSource
:AsynchronousSource rdf:type owl:Class ;
                    rdfs:subClassOf :DataSource ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :refreshTime ;
                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onDataRange xsd:integer
                                    ] ;
                    owl:disjointWith :StreamSource ,
                                     :SynchronousSource ;
                    rdfs:label "Asynchronous Source"@en ;
                    <http://www.w3.org/2004/02/skos/core#definition> "Data source that is not synchronized when it is sent or received"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#BaseExpression
:BaseExpression rdf:type owl:Class ;
                rdfs:subClassOf :EvaluableExpression ;
                owl:disjointWith :RecursiveExpression ;
                rdfs:label "Base Expression"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "Expression that cannot have nested functions and expects no parameters, only returns"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#BlankNode
:BlankNode rdf:type owl:Class ;
           rdfs:subClassOf :Node ;
           rdfs:label "Blank Node" ;
           <http://www.w3.org/2004/02/skos/core#definition> "Unique node with no intrinsic name that is not a reference to a resource or a literal"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#Constant
:Constant rdf:type owl:Class ;
          rdfs:subClassOf :BaseExpression ;
          rdfs:label "Constant"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> "Expression that contains a constant value"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#DataAccess
:DataAccess rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :URI ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:anyURI
                            ] ;
            rdfs:label "Data Access"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "Description of how to retrieve the data from its source"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#DataSource
:DataSource rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :hasDataAccess ;
                              owl:someValuesFrom :DataAccess
                            ] ;
            rdfs:label "Data Source"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "Description of the retrieved data"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#DataSourceAccess
:DataSourceAccess rdf:type owl:Class ;
                  rdfs:subClassOf :BaseExpression ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasDataSource ;
                                    owl:someValuesFrom :DataSource
                                  ] ;
                  rdfs:label "Data Source Access"@en ;
                  <http://www.w3.org/2004/02/skos/core#definition> "Expression with a value retrieved from the source data"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#EvaluableExpression
:EvaluableExpression rdf:type owl:Class ;
                     rdfs:label "Evaluable Expression"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Element that contains the information to form objects, predicates and subjects"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#EventSource
:EventSource rdf:type owl:Class ;
             rdfs:subClassOf :AsynchronousSource ;
             owl:disjointWith :PeriodicSource ;
             rdfs:label "Event Source"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> "Type of data access that retrieves data when a package arrives"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#GraphSource
:GraphSource rdf:type owl:Class ;
             rdfs:subClassOf :DataSource ;
             owl:disjointWith :TabularSource ,
                              :TextSource ,
                              :TreeSource ;
             rdfs:label "Graph Source"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> "Data source that is stored as a graph"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#HTMLSource
:HTMLSource rdf:type owl:Class ;
            rdfs:subClassOf :TreeSource ;
            owl:disjointWith :JSONSource ,
                             :XMLSource ;
            rdfs:label "HTML Source"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "Data stored in HTML format"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#JSONSource
:JSONSource rdf:type owl:Class ;
            rdfs:subClassOf :TreeSource ;
            owl:disjointWith :XMLSource ;
            rdfs:label "JSON Source"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "Data stored in JSON format"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#LinkingExpression
:LinkingExpression rdf:type owl:Class ;
                   rdfs:subClassOf :RecursiveExpression ;
                   rdfs:label "Linking Expression"@en ;
                   <http://www.w3.org/2004/02/skos/core#definition> "Expression that defines the link between two Nodes"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#Node
:Node rdf:type owl:Class ;
      rdfs:subClassOf :ResourceMap ;
      rdfs:label "Node" ;
      <http://www.w3.org/2004/02/skos/core#definition> "Union of blank node and reference node"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#Ontology
:Ontology rdf:type owl:Class ;
          rdfs:subClassOf owl:Ontology ;
          rdfs:label "Ontology"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> "Ontology used to guide the data translation"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#PeriodicSource
:PeriodicSource rdf:type owl:Class ;
                rdfs:subClassOf :AsynchronousSource ;
                rdfs:label "Periodic Source"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "Data source that is retrieven repeatedly in a period of time"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#RecursiveExpression
:RecursiveExpression rdf:type owl:Class ;
                     rdfs:subClassOf :EvaluableExpression ;
                     rdfs:label "Recursive Expression"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Expression that can expect one or more parameters and nested functions"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#ReferenceNode
:ReferenceNode rdf:type owl:Class ;
               rdfs:subClassOf :Node ;
               rdfs:label "Reference Node" ;
               <http://www.w3.org/2004/02/skos/core#definition> "Node that references an IRI"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#ResourceMap
:ResourceMap rdf:type owl:Class ;
             owl:equivalentClass rdfs:Resource ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :hasEvaluableExpression ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :EvaluableExpression
                             ] ;
             rdfs:label "Resource Map"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#StatementMap
:StatementMap rdf:type owl:Class ;
              rdfs:subClassOf rdf:Statement ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasObject ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :ResourceMap
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasPredicate ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :ReferenceNode
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasSubject ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :Node
                              ] ;
              rdfs:label "Statement Map"@en ;
              <http://www.w3.org/2004/02/skos/core#definition> "Statement that defines for a triple its subject, predicate, object, and datatype, language and assigned named graph if applicable"@en .


###  http://vocab.linkeddata.es/def/conceptual-mapping#StreamSource
:StreamSource rdf:type owl:Class ;
              rdfs:subClassOf :DataSource ;
              owl:disjointWith :SynchronousSource ;
              rdfs:label "Stream Source"@en ;
              <http://www.w3.org/2004/02/skos/core#definition> "Data that is continuously retrieved"@en .


